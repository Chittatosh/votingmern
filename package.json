{
  "name": "votingmern",
  "version": "1.0.0",
  "description": "Voting App for freeCodeCamp",
  "main": "dist/server/index.js",
  "author": "chittatosh",
  "license": "ISC",
  "engines": {
    "node": "8.9.4",
    "yarn": "1.5.1"
  },
  "scripts": {
    "start": "node bs/server",
    "heroku-postbuild": "webpack && rm -rf bs && mkdir bs && mkdirp bs/server && babel server -d bs/server && mkdirp bs/common && babel common -d bs/common",
    "serve": "nodemon --exec babel-node server --ignore dist/ --ignore tests/",
    "dev": "webpack -wd",
    "hotserver": "rm -rf serverdist/ && webpack --config ./server.webpack.config.js",
    "hotclient": "webpack-dev-server --config ./client.webpack.config.js",
    "webpack": "webpack --json > stats.json",
    "hot": "npm-run-all --parallel hotserver hotclient",
    "test": "jest --watch"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.18.2",
    "connect-ensure-login": "^0.1.1",
    "connect-mongo": "^2.0.0",
    "cross-fetch": "^1.1.1",
    "dotenv": "^5.0.1",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "heroku-ssl-redirect": "^0.0.4",
    "mkdirp": "^0.5.1",
    "mongoose": "^4.13.5",
    "npm-run-all": "^4.1.2",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth20": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "pretty-error": "^2.1.1",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "start-server-webpack-plugin": "^2.2.5",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.1",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "jest": "^22.0.5",
    "jest-fetch-mock": "^1.4.0",
    "nodemon": "^1.12.1",
    "redux-mock-store": "^1.4.0",
    "reload-server-webpack-plugin": "^1.0.1"
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "./tests/setupJest.js"
    ]
  }
}
